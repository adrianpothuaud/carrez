{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "rental-estimation",
    "description": "A rental property from leboncoin",
    "type": "object",
    "properties": {
        "price": {
            "description": "Price of the product",
            "type": "number"
        },
        "address": {
            "type": "string"
        },
        "zip": {
            "type": "integer",
            "description": "zip code of the city where the product stands"
        },
        "city": {
            "type": "string"
        },
        "type": {
            "type": "string",
            "description": "type of rental property"
        },
        "rooms": {
            "type": "integer",
            "description": "number of rooms of this rental"
        },
        "surface-area": {
            "type": "integer",
            "description": "surface area of the property"
        },
        "quality": {
            "type": "string",
            "description": "estimate the quality of this good compared to similar"
        },
        "elevator": {
            "type": "boolean"
        },
        "balcon": {
            "is": {
              "type": "boolean"
            },
            "surface-area": {
              "type": "integer"
            }
        },
        "terrasse": {
            "is": {
              "type": "boolean"
            },
            "surface-area": {
              "type": "integer"
            }
        },
        "cave": {
            "is": {
              "type": "boolean"
            },
            "number": {
              "type": "integer"
            }
        },
        "parking": {
            "is": {
              "type": "boolean"
            },
            "number": {
              "type": "integer"
            }
        },
        "service": {
            "is": {
              "type": "boolean"
            },
            "number": {
              "type": "integer"
            }
        },
        "exception": {
            "type": "boolean"
        },
        "renovation-a-prevoir": {
            "type": "boolean"
        },
        "commun-renov": {
            "type": "boolean"
        },
        "ravalement-recent": {
            "type": "boolean"
        },
        "year-construct": {
            "type": "integer"
        },
    "required": ["id", "price", "zip", "surface-area"]
}
