{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "rental-leboncoin",
    "description": "A rental property from leboncoin",
    "type": "object",
    "properties": {
        "price": {
            "description": "Price of the product",
            "type": "number"
        },
        "address": {
            "type": "string",
            "description": "address of the property"
        },
        "zip": {
            "type": "integer",
            "description": "zip code of the city where the product stands"
        },
        "type": {
            "type": "string",
            "description": "type of rental property"
        },
        "rooms": {
            "type": "integer",
            "description": "number of rooms of this rental"
        },
        "surface-area": {
            "type": "integer",
            "description": "surface area of the property"
        },
        "ges": {
            "type": "character",
            "description": "ges identifier"
        },
        "energetic-class": {
            "type": "character",
            "description": "surface area of the property"
        }
    },
    "required": ["price", "address", "zip", "surface-area"]
}
